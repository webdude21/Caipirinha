        Title="GameField" Height="326.36" Width="528.452">
	<Window.Resources>		
		<ResourceDictionary x:Name="Backgrounds">
			<ImageBrush x:Key="clearing" ImageSource="Backgrounds_Images\clearing.png" Stretch="UniformToFill"/>
